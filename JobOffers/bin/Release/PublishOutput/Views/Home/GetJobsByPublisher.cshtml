@model IEnumerable<JobOffers.Models.JobViewModel>

@foreach (var gr in Model)
{
<h3>@gr.JobTitle</h3>
    <table class="table table-bordered table-hover">
        <tr>
            <th>
                اسم الباحث
            </th>
            <th>
                نص الرسالة
            </th>
            <th>
                تاريخ الارسال
            </th>
        </tr>

        @foreach (var item in gr.Items)
        {
        <tr>
            <td>
                @item.User.UserName
            </td>
            <td>
                @item.Message
            </td>
            <td>
                @item.ApplyDate
            </td>
        </tr>
        }

    </table>
}